{
  "name": "redux-boilerplate",
  "version": "0.0.0",
  "author": "Gustavo Perenciolo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/perenciolo/redux-toolkit-boilerplate/issues"
  },
  "homepage": "https://github.com/perenciolo/redux-toolkit-boilerplate#readme",
  "scripts": {
    "dev": "vite",
    "test": "yarn run jest",
    "test:watch": "yarn test --runInBand --watch",
    "test:coverage": "yarn test --coverage",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.21.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/dom": "^8.7.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-react": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.4",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^4.12.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "jest-circus": "^27.2.4",
    "jest-junit": "^12.3.0",
    "jest-scss-transform": "^1.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^11.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "jest-sonar-reporter.xml",
    "indent": 4
  },
  "pre-commit": "lint"
}